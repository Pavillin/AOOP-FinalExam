<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CarViewController">
   <children>
      <ComboBox fx:id="brandComboBox" layoutX="26.0" layoutY="44.0" prefHeight="39.0" prefWidth="210.0" promptText="Select Brand" />
      <TableView fx:id="tableView" layoutX="28.0" layoutY="169.0" prefHeight="200.0" prefWidth="544.0">
        <columns>
          <TableColumn fx:id="yearColumn" prefWidth="75.0" text="Year" />
          <TableColumn fx:id="brandColumn" prefWidth="93.0" text="Brand" />
            <TableColumn fx:id="modelColumn" prefWidth="163.0" text="Model" />
            <TableColumn fx:id="kilometersColumn" prefWidth="104.0" text="Kilometers" />
            <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="108.0" text="Price" />
        </columns>
      </TableView>
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="326.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="sellButton" layoutX="99.0" layoutY="114.0" mnemonicParsing="false" text="Sell Car" />
      <Label layoutX="28.0" layoutY="369.0" text="Value of inventory showing:" />
      <Label fx:id="invValueLabel" layoutX="183.0" layoutY="369.0" />
   </children>
</AnchorPane>
